                    <tr>
                        <td>${post.id}</td>
                        <td>${post.category}</td>
                        <td class="text-start">
                            <a href="${pageContext.request.contextPath}/board/view?id=${post.id}">
                                    ${post.title}
                            </a>
                        </td>
                        <td>${post.writer}</td>
                        <td>${post.createdAt}</td>
                    </tr>
                </c:forEach>
